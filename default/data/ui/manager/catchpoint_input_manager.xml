<endpoint name="data/inputs/catchpoint_service">
	<header>Catchpoint is dope af</header>
	<breadcrumb>
    	<parent hidecurrent="False">datainputstats</parent>
    	<name>Catchpoint Modular Input</name>
  </breadcrumb>
  <elements>
  	<element name="sourceFields" type="fieldset">
  		<view name="list" />
		<view name="edit" />
		<view name="create" />
		<elements>
			<element name="name" label="Catchpoint Modular Input Name">
				<view name="list" />
				<view name="create" />
				<key name="exampleText">Name of this REST API Input</key>
			</element>
			<element name="rest_endpoint" label="This is the carter">
				<view name="edit" />
				<view name="create" />
		  		<view name="list" />
		  		<key name="exampleText">Neeeeext</key>
		  	</element>
		  	<element name="duration" label="My new label">
		  		<view name="edit" />
				<view name="create" />
		  		<view name="list" />
				<key name="exampleText">Example Example </key>
		  	</element>

		  	<element name="drop_down" type="select" label="Element Selection">
				<onChange>
					<key name="_action">showonly</key>
					<group_set>
						<group name="element_one" />
						<group name="element_two" />
					</group_set>
					<key name="element_one">element_one</key>
					<key name="element_two">element_two</key>
				</onChange>
				<view name="edit" />
				<view name="create" />
				<key name="exampleText"> Select an element: one or two.</key>
				<options>
					<opt value="element_one" label="Element One" />
					<opt value="element_two" label="Element Two" />
				</options>
		  	</element>

		  	<element name="element_one" type="fieldset" />
		  	<element name="element_two" type="fieldset">
		  		<view name="edit" />
		  		<view name="create" />
		  		<elements>
			  		<element name="element_two_text" type="textfield" label="Element Two">
			  			<view name="edit" />
			  			<view name="create" />
			  			<key name="exampleText"> You have selected Example Two!</key>
			  		</element>
			  	</elements>
		  	</element>
		</elements>
  	</element>
	<element name="eai:acl.app" label="App">
      <view name="list"/>
      <key name="processValueList">entity['eai:acl']['app'] or ""</key>
    </element>
  </elements>
</endpoint>